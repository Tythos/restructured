{
  "name": "rst.js",
  "description": "A reStructuredText parser for JavaScript",
  "version": "0.0.0",
  "author": "seikichi",
  "bugs": {
    "url": "https://github.com/seikichi/rst.js/issues"
  },
  "dependencies": {
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.13.1",
    "eslint-watch": "^2.1.4",
    "espower-babel": "^4.0.1",
    "foreman": "^1.4.1",
    "mocha": "^2.3.4",
    "pegjs": "^0.9.0",
    "power-assert": "^1.2.0",
    "watch": "^0.17.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/seikichi/rst.js",
  "keywords": [
    "reST",
    "reStructuredText",
    "rst"
  ],
  "license": {
    "type": "MIT",
    "url": "http://seikichi.mit-license.org/"
  },
  "main": "lib/rst.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/seikichi/rst.js.git"
  },
  "scripts": {
    "build": "npm run build:parser && babel src --source-maps inline --out-dir lib",
    "build:parser": "pegjs src/parser.pegjs lib/parser.js",
    "lint": "eslint src test",
    "test": "mocha --compilers js:espower-babel/guess test/*.js",
    "watch": "nf start",
    "watch:build": "babel src --watch --source-maps inline --out-dir lib",
    "watch:build:parser": "watch 'npm run -s build:parser' src -d --filter=config/filter.js",
    "watch:lint": "esw --watch src test",
    "watch:test": "watch 'npm -s run test' src test -d"
  }
}
