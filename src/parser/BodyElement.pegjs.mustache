BodyElementExceptBlockQuote =
  blanks:BlankLines? element:(
    Comment /
    LiteralBlock /
    BulletList /
    EnumeratedList /
    DefinitionList /
    Paragraph
  ) {
    return element.withBlankLines(blanks, [], options);
  }

BodyElement =
  element:(BodyElementExceptBlockQuote / BlockQuote) {
    return element.withLoc(location, options);
  }
