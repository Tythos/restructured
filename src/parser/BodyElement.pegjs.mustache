BodyElementExceptBlockQuote =
  blanklines:BlankLines? element:(
    Directive /
    Comment /
    LiteralBlock /
    BulletList /
    EnumeratedList /
    DefinitionList /
    LineBlock /
    Paragraph
  ) {
    element.blanklines = blanklines || [];
    return element;
  }

BodyElement =
  element:(BodyElementExceptBlockQuote / BlockQuote) {
    return element;
  }
